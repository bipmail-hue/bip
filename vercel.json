{
  "buildCommand": "cd homebanking-frontend && npm install && npm run build",
  "outputDirectory": "homebanking-frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://bip-production.up.railway.app/api"
  }
}
